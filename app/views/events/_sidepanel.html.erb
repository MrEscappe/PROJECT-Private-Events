<div class="row">
    <div class="col-4">
        <h2>Past Events</h2>
    

        <div class="">
            <% @events.past.each do |event| %>
                <div class="">
                    <h3><%= event.title %></h3>
                    <p><%= event.description %></p>
                    <p><%= event.date %></p>
                </div>
            <% end %>
        </div>
    </div>
    
</div>